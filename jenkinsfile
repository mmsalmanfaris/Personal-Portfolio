pipeline{
    agent any


    stages{
        stage('Version'){
            steps{
                    sh 'node --version'
                    sh 'npm --version'
            }
        }
        stage('Dependency'){
            steps{
                sh 'npm install'
            }
        }

        stage('Build'){
            steps{
                sh 'npm run build'
            }
        }
    }
}
